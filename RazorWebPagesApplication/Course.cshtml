@page
@model RazorWebPagesApplication.CourseModel
@{
}
    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title></title>
    </head>
    <body>
        <p>Popular Courses this Month</p>
        @{ var courses = new List<string>()
            {
               "Computer Science",
               "Data Science",
               "Cybersecurity"
            };
            <ul>
                @foreach (string course in courses)
                {
                    <li>@course</li>
                }
            </ul>
         }
    </body>
    </html>